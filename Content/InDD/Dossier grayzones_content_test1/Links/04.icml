						<Link Self="u4098" AssetURL="$ID/" AssetID="$ID/" LinkResourceURI="file:/Users/lenarobin/Documents/TRAVAIL/INC/TOD17-Gray-Zones/InDD/Dossier%20grayzones_content_test1/Links/Leger%20IMAGE.jpg" LinkResourceFormat="$ID/JPEG" StoredState="Normal" LinkClassID="35906" LinkClientID="257" LinkResourceModified="false" LinkObjectModified="false" ShowInUI="true" CanEmbed="true" CanUnembed="true" CanPackage="true" ImportPolicy="NoAutoImport" ExportPolicy="NoAutoExport" LinkImportStamp="file 130923978350000000 286992" LinkImportModificationTime="2015-11-19T10:10:35" LinkImportTime="2015-11-19T12:13:25" LinkResourceSize="0~46110"/>
